cc_binary {
    name: "irqbalance",
    proprietary: true,
    init_rc: ["irqbalance.rc"],
    shared_libs: [
        "libcutils",
        "liblog",
        "libutils",
    ],

    srcs: [
        "irqbalance.c",
        "configparser.c"
    ],

    cflags: [
        "-Wall",
        "-Werror",
        "-Wno-unused-parameter",
        "-Wno-unused-function",
        "-Wno-unused-variable",
    ],
}
